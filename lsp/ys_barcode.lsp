(defun ins_str(strSub basepoint / x y base_txt);对输入的字符根据基点插入其barcode,并在其下写出文字.
  	(cond
  	    ((equal strSub "1")(ins_1 basepoint))
	    ((equal strSub "2")(ins_2 basepoint))
	    ((equal strSub "3")(ins_3 basepoint))
	    ((equal strSub "4")(ins_4 basepoint))
	    ((equal strSub "5")(ins_5 basepoint))
	    ((equal strSub "6")(ins_6 basepoint))
	    ((equal strSub "7")(ins_7 basepoint))
	    ((equal strSub "8")(ins_8 basepoint))
	    ((equal strSub "9")(ins_9 basepoint))
	    ((equal strSub "0")(ins_0 basepoint))
	    ((equal strSub "A")(ins_a basepoint))
	    ((equal strSub "B")(ins_b basepoint))
	    ((equal strSub "C")(ins_c basepoint))
	    ((equal strSub "D")(ins_d basepoint))
	    ((equal strSub "E")(ins_e basepoint))
	    ((equal strSub "F")(ins_f basepoint))
	    ((equal strSub "G")(ins_g basepoint))
	    ((equal strSub "H")(ins_h basepoint))
	    ((equal strSub "I")(ins_i basepoint))
	    ((equal strSub "J")(ins_j basepoint))
	    ((equal strSub "K")(ins_k basepoint))
	    ((equal strSub "L")(ins_l basepoint))
	    ((equal strSub "M")(ins_m basepoint))
	    ((equal strSub "N")(ins_n basepoint))
	    ((equal strSub "O")(ins_o basepoint))
	    ((equal strSub "P")(ins_p basepoint))
	    ((equal strSub "Q")(ins_q basepoint))
	    ((equal strSub "R")(ins_r basepoint))
	    ((equal strSub "S")(ins_s basepoint))
	    ((equal strSub "T")(ins_t basepoint))
	    ((equal strSub "U")(ins_u basepoint))
	    ((equal strSub "V")(ins_v basepoint))
	    ((equal strSub "W")(ins_w basepoint))
	    ((equal strSub "X")(ins_x basepoint))
	    ((equal strSub "Y")(ins_y basepoint))
	    ((equal strSub "Z")(ins_z basepoint))
	    ((equal strSub "-")(ins_hengxian basepoint))
	    ((equal strSub "*")(ins_stop_start basepoint))
	    ;(t (progn(print"\n错误,退出.")(exit)))
	)
	(setq x (car basepoint))
  	(setq y (cadr basepoint))
  	(setq base_txt(list (+ x 1.275)(- y 1.6)))
  	(command "text" "j" "mc" base_txt "1"  "0" strSub)
)


(defun c:ystxt( / str size num_field stringlen cood_x cood_y startpoint
	         base_str endpoint strSub axis_x lbpoint len_x len_y
	      	 rtpoint num base_end)
  	(setvar"osmode" 0)
	(setvar"orthomode" 0)

  	(command "-layer" "n" "yuansheng_text" "s" "yuansheng_text" "")
        (command "zoom" "-127,-127" "127,127")
  	
  	(setq str(getstring"\n请输入字符串以生成barcode(条码):"))
  	(setq txt (getstring T "光罩光称及台头:"))
	(setq str(strcase str))	  	 

  	(setq stringlen(strlen str));计算字符串的长度.
  	(if(equal stringlen 0)
	   (progn
	     (print"\n输入错误")
	     (exit)
	    )
	)
  	(setq cood_x (- 28.0 (/ (+ (* stringlen 2.55) (* 0.2 (- stringlen 1))) 2)));计算barcode左下基点x坐标.
  	(setq cood_y 54.0);计算barcode左下基点y坐标.
  	(setq num 1);对字符串逐一取字符时的计数.
  
  	(setq startpoint(list (- cood_x 2.75) cood_y));插入起点barcode"*"号.
  	(ins_str "*" startpoint);(ins_stop_start startpoint)
  	(setq base_str (list (+ 2.55 (car startpoint))(- cood_y 1.6)));计算文字插入基点.
  	;(command "text" "j" "mr" base_str "1"  "0" "*")
  
  	(setq endpoint(list (+ 28.0 0.2 (/ (+ (* stringlen 2.55) (* 0.2 (- stringlen 1))) 2)) cood_y));插入终点barcode"*".
  	(ins_str "*" endpoint);(ins_stop_start endpoint)
  	(setq base_end(list(car endpoint)(- (cadr endpoint) 1.6)))
  	;(command "text" "j" "ml" base_end "1"  "0" "*")
  
  	(while(<= num stringlen);加中间的barcodes.
	      (setq strSub(substr str num 1))
	      (setq axis_x(+ cood_x (* (- num 1) 2.75)))
	      (setq basepoint (list axis_x cood_y))
	      (ins_str strSub basepoint)
	      (setq num (+ num 1))
	)
  	(setq lbpoint(list (- cood_x 6.25)  cood_y));插入大长方形
  	(setq len_x (+ (- (* stringlen 2.75) 0.2) 12.5))
  	(setq len_y 2.0)
  	(setq rtpoint (list (+ (- cood_x 6.25) len_x) (+ cood_y 3.0)))
  	(command "rectangle" lbpoint rtpoint)

  	(ystxt txt);加清溢标志及日期.
        (ys_frame);加边框上的5mm厚的黑边图形.  
  	(print"\n----->运行完毕.")
  	(prompt "\n**************************")
  	(prompt "\n*特别注意:别忘了加切割框!*")
  	(prompt "\n**************************")
)

